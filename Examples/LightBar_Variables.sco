USE LED1
USE LED2
USE LED3
USE LED4
USE LED5
USE LIGHT 
VAR START = 100
VAR STEP = 20
IF(LIGHT < (START - STEP)){
	LED1 ON
}ELSE{
	LED1 OFF
}
IF(LIGHT < (START - STEP*2)){
	LED2 ON
}ELSE{
	LED2 OFF
}
IF(LIGHT < (START - STEP*3)){
	LED3 ON
}ELSE{
	LED3 OFF
}
IF(LIGHT < (START - STEP*3.5)){
	LED4 ON
}ELSE{
	LED4 OFF
}
IF(LIGHT < (START - STEP*4.5)){
	LED5 ON
}ELSE{
	LED5 OFF
}